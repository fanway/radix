use std::collections::HashMap;


struct NaiveRadixTreeNode<'a, T> {
    next: HashMap<&'a str, &'a NaiveRadixTreeNode<'a, T>>,
    element: T
}

impl<'a, T> NaiveRadixTreeNode<'a, T> {
    fn new() -> Self {
        Self {
            next: HashMap::new(),
            element: T::default()
        }
    }
}

fn main() {
    let root = NaiveRadixTreeNode::new();
}
